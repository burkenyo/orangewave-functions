{
  "packageManager": "pnpm@7.27.1",
  "name": "cert-fresh",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "exec": "pnpm build && pnpm run-only",
    "run-only": "node --experimental-specifier-resolution=node out/index.js",
    "scaffold": "openapi --input ionos-dns-api.yaml --output src/ionos-dns-api --client axios --postfix Client --indent 2"
  },
  "dependencies": {
    "@azure/identity": "^3.1.3",
    "@azure/keyvault-certificates": "^4.6.0",
    "@azure/keyvault-secrets": "^4.6.0",
    "acme-client": "^5.0.0",
    "axios": "^1.3.3",
    "dotenv": "^16.0.3",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "openapi-typescript-codegen": "^0.23.0",
    "typescript": "^4.9.5"
  }
}
